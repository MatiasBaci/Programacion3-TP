<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="331.0" prefWidth="334.0" spacing="20.0" styleClass="bodyFondoSeleccionPokemon" stylesheets="@StyleCss/fondo_seleccion_nombre.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="orgFiuba.tpjava.Controller.SeleccionarNombreJugadoresController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <Pane prefHeight="91.0" prefWidth="299.0">
      <children>
         <ImageView translateX="160" translateY="-200" fitHeight="300.0" fitWidth="600.0" layoutX="37.0" layoutY="10.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@Imagenes/Iconos/SeleccionDeNombre.png" />
            </image>
         </ImageView>
      </children></Pane>
   <TextField fx:id="nombreJugador" maxWidth="-Infinity" onAction="#siguienteVentana" prefHeight="40.0" prefWidth="266.0" promptText="Ingrese su nombre">
      <font>
         <Font size="18.0" />
      </font>
   </TextField>
   <Button mnemonicParsing="false" onAction="#siguienteVentana" text="Continuar">
      <font>
         <Font size="18.0" />
      </font>
   </Button>
   <Label fx:id="label" />
</VBox>
